<UserControl x:Class="ReportWizardPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:hw="http://schemas.hpod9.com/haley/wpf"
             xmlns:hm="http://schemas.hpod9.com/haley/mvvm"
             xmlns:local="clr-namespace:EZLogger"
             mc:Ignorable="d"
             d:DesignHeight="450" Width="795">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/EZLogger;component/Resources/EzLoggerRD.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid Background="White" Margin="0,0,-5,0">
        <local:ReadOnlyField HorizontalAlignment="Left" VerticalAlignment="Top" LabelText="Bryan Lundeen"/>
        <local:UserDataEntryField HorizontalAlignment="Left" Margin="0,100,0,0" VerticalAlignment="Top"/>
        <local:ButtonGrouped HorizontalAlignment="Left" Margin="5,190,0,0" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
